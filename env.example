# Google Cloud Project Configuration
TF_VAR_project_id=your-gcp-project-id

# Deployment Configuration
TF_VAR_region=us-central1
TF_VAR_bucket_location=US

# Function Configuration
TF_VAR_function_name=my-python-function
TF_VAR_function_description=Python HTTP Cloud Function deployed via Terraform
TF_VAR_runtime=python312
TF_VAR_entry_point=hello_http

# Scaling Configuration
TF_VAR_max_instance_count=3
TF_VAR_min_instance_count=0
TF_VAR_available_memory=256M
TF_VAR_timeout_seconds=60

# Security Configuration
TF_VAR_allow_unauthenticated=true
TF_VAR_ingress_settings=

# Optional: Service Account Email
# TF_VAR_service_account_email="your-service-account@your-project.iam.gserviceaccount.com"

# Optional: Bucket Settings
# TF_VAR_bucket_force_destroy=true
